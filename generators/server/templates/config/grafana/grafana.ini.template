[paths]
logs = /log
data = /data
plugins = /plugins

[log]
level = Warn

[session]
cookie_name = grafana_sess_SITE_ID

[server]
domain = SITE_FQDN
enable_gzip = true
root_url = %(protocol)s://SITE_FQDN:HTTP_PORT/SITE_ID/

[auth.anonymous]
enabled = true

[auth.proxy]
enabled = true

[security]
disable_gravatar = true

[snapshots]
external_enabled = false

[users]
allow_sign_up = false
allow_org_create = false